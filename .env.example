# Domaine principal utilisé pour générer le certificat wildcard
DOMAIN=local.example.com

# Sous-domaines utilisés pour les services
PORTAINER_HOSTNAME=portainer
OAUTH2_PROXY_HOSTNAME=auth

# Identifiants pour le DNS challenge ACME (Cloudflare)
CLOUDFLARE_EMAIL=you@example.com
CLOUDFLARE_API_TOKEN=changeme

# Mot de passe pour l’administrateur Keycloak
KEYCLOAK_ADMIN_PASSWORD=changeme

# Cookie secret utilisé par oauth2-proxy (doit être 32 bytes base64)
OAUTH2_PROXY_COOKIE_SECRET=changeme
